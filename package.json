{
  "name": "m12e",
  "version": "0.0.1",
  "scripts": {
    "assets": "webpack --color --display-max-modules 0",
    "assets:watch": "yarn assets -d -w  --color true --display minimal",
    "build": "yarn clean && cross-env NODE_ENV=production yarn assets -p",
    "clean": "rm -rf dist",
    "serve": "webpack-serve ./webpack.config.js --port 3000 --open",
    "serve:dev": "cross-env NODE_ENV=development yarn serve",
    "serve:prod": "cross-env NODE_ENV=production yarn serve"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "engines": {
    "node": ">= 11.6",
    "npm": ">= 6.5",
    "yarn": ">= 1.3"
  },
  "license": "MIT",
  "browserslist": "last 3 versions",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "^7.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "autoprefixer": "^9.4.6",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "bulma": "^0.7.2",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.1",
    "eslint-plugin-prettier": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.13",
    "postcss-loader": "^3.0.0",
    "postcss-uncss": "^0.16.1",
    "prettier": "^1.16.0",
    "sass-loader": "^7.1.0",
    "terser-webpack-plugin": "^1.2.1",
    "uncss": "^0.16.2",
    "uncss-webpack-plugin": "^0.2.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-serve": "^2.0.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "airbnb-base"
    ],
    "plugins": [
      "import",
      "html",
      "prettier"
    ],
    "rules": {}
  },
  "prettier": {
    "arrowParens": "avoid",
    "plugins": [
      "all"
    ],
    "printWidth": 102,
    "proseWrap": "always",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  }
}
