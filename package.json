{
  "name": "m5ls5e.com",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "preview": "vite preview",
    "start": "wrangler pages dev -- vite serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": {
    "name": "Michael Sprague",
    "email": "mikesprague@gmail.com"
  },
  "repository": {
    "url": "https://github.com/mikesprague/m5ls5e-com"
  },
  "engines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.2.1",
    "@fortawesome/free-brands-svg-icons": "6.2.1",
    "bulma": "0.9.4",
    "cheerio": "1.0.0-rc.12"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@babel/eslint-plugin": "7.19.1",
    "@babel/preset-env": "7.20.2",
    "@fullhuman/postcss-purgecss": "5.0.0",
    "autoprefixer": "10.4.13",
    "cssnano": "5.1.14",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-stylelint": "17.1.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "postcss": "8.4.19",
    "postcss-cli": "10.1.0",
    "prettier": "2.8.1",
    "prettier-eslint": "15.0.1",
    "sass": "1.56.2",
    "stylelint": "14.16.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-prettier": "2.0.0",
    "vite": "3.2.5",
    "wrangler": "2.6.2"
  },
  "peerDependencies": {
    "@miniflare/queues": "2.8.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": {
    "development": [
      "last 1 version"
    ],
    "production": [
      "> 1%",
      "last 2 versions",
      "not ie <= 10"
    ]
  },
  "eslintConfig": {
    "settings": {
      "jest": {
        "version": 28
      }
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "prettier",
      "stylelint"
    ],
    "plugins": [
      "babel",
      "compat",
      "html",
      "import",
      "prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "ecmaVersion": 11,
      "sourceType": "module",
      "ecmaFeatures": {
        "impliedStrict": true,
        "modules": true
      }
    },
    "rules": {
      "node/no-unpublished-import": "warn",
      "node/no-missing-import": "warn",
      "no-console": "warn"
    },
    "ignorePatterns": [
      "dist/",
      ".devcontainer/",
      ".github/",
      ".history/",
      "node_modules/"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "tabWidth": 2
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
